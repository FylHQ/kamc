{
    "java.configuration.updateBuildConfiguration": "automatic",
    "saveAndRun": {
        "commands": [
          {
            "match": "\\.(js|vue)$",
            "cmd": "cd ${workspaceRoot}/src/main/resources/static ; npm run webpack",
            "useShortcut": false,
            "silent": false
          }
        ]
      },
      "eslint.enable": false,
      "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
      },
      "python.linting.pylintEnabled": true,
      "editor.tabSize": 3
      
}